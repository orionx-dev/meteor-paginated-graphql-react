{
  "name": "@orionx-dev/meteor-paginated-graphql-react",
  "version": "1.5.3",
  "main": "lib/index.js",
  "repository": "http://github.com/orionsoft-dev/meteor-paginated-graphql-react",
  "author": "nicolaslopezj & Joadr",
  "license": "MIT",
  "scripts": {
    "build": "babel ./src --out-dir ./lib && npm run build-styles",
    "build-styles":
      "find src/ -name '*.less' ! -name '*import.less' -exec lessc {} \\; > ./lib/styles.css",
    "prepublish": "npm run build",
    "test": "npm run lint && jest",
    "watch": "node watch.js",
    "test-watch": "jest --watch",
    "lint": "standard \"src/**/*.js\""
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "colors": "^1.1.2",
    "eslint": "^4.7.0",
    "eslint-config-orionsoft": "^1.2.1",
    "graphql-tag": "^2.12.6",
    "jest-cli": "^21.1.0",
    "less": "^2.7.2",
    "standard": "^10.0.3"
  },
  "peerDependencies": {
    "graphql-tag": "^2.12.6",
    "react": "^16.8.0",
    "@apollo/client": "^3.7.3"
  },
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "dot-object": "^1.6.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react-icons": "^4.7.1",
    "simple-react-form": "^2.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": ["/node_modules/", "/lib"]
  },
  "publishConfig": {
    "registry":"https://npm.pkg.github.com/orionx-dev"
  }
}
